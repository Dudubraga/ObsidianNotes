filters:
  and:
    - file.tags.containsAny("graduação")
views:
  - type: table
    name: Table
    filters:
      and:
        - file.tags.containsAny("unicap", "maratona", "pibic")
    order:
      - file.name
      - file.folder
      - tags
      - file.mtime
      - file.size
    sort:
      - property: tags
        direction: DESC
      - property: file.name
        direction: ASC
    columnSize:
      file.name: 279
      note.tags: 345
      file.mtime: 152
